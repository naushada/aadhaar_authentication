  global staging_wi_pname_v16 
  set arg {type posh}
  set idx 0
  set str ""
  set attr [lindex $staging_wi_pname_v16 $offset]

  foreach val $attr {
   set str [concat $str [lindex $arg $idx]=[.$val get]]
   incr idx
  }

  set str [regsub -all {\s+} $str ,]
  set str "bio={$str}"
  return $str

};#/*staging_get_bio_attr_value*/

